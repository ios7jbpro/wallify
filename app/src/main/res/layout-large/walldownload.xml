<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/backgroundviolent"
	android:orientation="horizontal">

	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="horizontal">

		<LinearLayout
			android:layout_width="250dp"
			android:layout_height="match_parent"
			android:background="@color/backgroundviolent"
			android:orientation="vertical">

			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@color/backgroundviolent"
				android:gravity="center_vertical"
				android:orientation="horizontal"
				android:padding="8dp">

				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Fetching..."
					android:textColor="@color/textprimary"
					android:textSize="18sp"
					android:textStyle="bold" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@color/backgroundviolent"
				android:orientation="vertical"
				android:padding="8dp">

				<TextView
					android:id="@+id/textViewCrop"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="8dp"
					android:layout_weight="1"
					android:background="@drawable/activetab"
					android:gravity="center_horizontal|center_vertical"
					android:padding="16dp"
					android:text="@string/CropWall"
					android:textColor="@color/textprimary"
					android:textSize="12sp"
					android:textStyle="bold" />

				<TextView
					android:id="@+id/button4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="8dp"
					android:layout_weight="1"
					android:background="@drawable/activetab"
					android:gravity="center_horizontal|center_vertical"
					android:padding="16dp"
					android:text="@string/ShareIMAGE"
					android:textColor="@color/textprimary"
					android:textSize="12sp"
					android:textStyle="bold" />

				<TextView
					android:id="@+id/button3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="8dp"
					android:layout_weight="1"
					android:background="@drawable/activetab"
					android:gravity="center_horizontal|center_vertical"
					android:padding="16dp"
					android:text="@string/ShareURL"
					android:textColor="@color/textprimary"
					android:textSize="12sp"
					android:textStyle="bold" />

				<TextView
					android:id="@+id/button1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="8dp"
					android:layout_weight="1"
					android:background="@drawable/activetab"
					android:gravity="center_horizontal|center_vertical"
					android:padding="16dp"
					android:text="@string/Download"
					android:textColor="@color/textprimary"
					android:textSize="12sp"
					android:textStyle="bold" />

				<TextView
					android:id="@+id/button2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:background="@drawable/activetab"
					android:gravity="center_horizontal|center_vertical"
					android:padding="16dp"
					android:text="@string/SetWall"
					android:textColor="@color/textprimary"
					android:textSize="12sp"
					android:textStyle="bold" />
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="@drawable/roundedbg"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">

			<LinearLayout
				android:id="@+id/linear5"
				android:layout_width="450dp"
				android:layout_height="match_parent"
				android:layout_weight="0"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:padding="8dp">

				<TextView
					android:id="@+id/textView2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="4dp"
					android:layout_marginBottom="6dp"
					android:text="@string/WallPreview"
					android:textColor="@color/textprimary"
					android:textSize="20sp"
					android:textStyle="bold" />

				<LinearLayout
					android:id="@+id/linearpreviewcard"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="6dp"
					android:background="@drawable/roundedbgviolent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical"
					android:padding="8dp">

					<LinearLayout
						android:id="@+id/linear20"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="16dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">

						<LinearLayout
							android:id="@+id/linear7"
							android:layout_width="170dp"
							android:layout_height="340dp"
							android:layout_marginRight="6dp"
							android:background="@drawable/roundedbgviolent"
							android:orientation="vertical">

							<FrameLayout
								android:id="@+id/linear8"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_horizontal"
								android:orientation="vertical">

								<TextView
									android:id="@+id/textview3"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_gravity="center_horizontal|bottom"
									android:layout_marginBottom="15dp"
									android:elevation="5dp"
									android:padding="8dp"
									android:text="Swipe to unlock"
									android:textColor="#FFFFFF"
									android:textSize="12sp" />

								<TextView
									android:id="@+id/textview2"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_gravity="center_horizontal"
									android:layout_marginTop="40dp"
									android:elevation="5dp"
									android:padding="8dp"
									android:text="9:30"
									android:textColor="@color/colorAccent"
									android:textSize="34sp"
									android:textStyle="bold" />

								<LinearLayout
									android:id="@+id/linear19"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:alpha="0.3"
									android:background="#000000"
									android:elevation="2dp"
									android:orientation="horizontal"
									android:padding="8dp" />

								<ImageView
									android:id="@+id/imageview1"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:scaleType="centerCrop"
									android:src="@drawable/default_image" />
							</FrameLayout>
						</LinearLayout>

						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="170dp"
							android:layout_height="340dp"
							android:layout_marginLeft="6dp"
							android:background="@drawable/roundedbgviolent"
							android:orientation="vertical">

							<FrameLayout
								android:id="@+id/linear10"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_horizontal"
								android:orientation="vertical">

								<LinearLayout
									android:id="@+id/LinearLayout1"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:elevation="2dp"
									android:orientation="vertical"
									android:paddingLeft="4dp"
									android:paddingTop="8dp"
									android:paddingRight="4dp">

									<LinearLayout
										android:id="@+id/previewStatusBar"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:orientation="horizontal">

										<TextView
											android:id="@+id/time2"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_gravity="center_horizontal"
											android:layout_marginLeft="4dp"
											android:text="9:30"
											android:textColor="#FFFFFF"
											android:textSize="10sp" />
									</LinearLayout>

									<LinearLayout
										android:id="@+id/Search_bg"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:layout_marginLeft="5dp"
										android:layout_marginTop="8dp"
										android:layout_marginRight="5dp"
										android:background="@drawable/roundedbg"
										android:gravity="center_vertical"
										android:orientation="horizontal">

										<ImageView
											android:id="@+id/ImageView4"
											android:layout_width="15dp"
											android:layout_height="15dp"
											android:layout_marginLeft="5dp"
											android:scaleType="fitCenter"
											android:src="@drawable/heeh_1" />

										<LinearLayout
											android:id="@+id/LinearLayout5"
											android:layout_width="match_parent"
											android:layout_height="wrap_content"
											android:gravity="right|center_vertical"
											android:orientation="horizontal"
											android:padding="8dp">

											<ImageView
												android:id="@+id/ImageView5"
												android:layout_width="15dp"
												android:layout_height="15dp"
												android:layout_marginRight="10dp"
												android:scaleType="fitCenter"
												android:src="@drawable/heeh_2" />

											<ImageView
												android:id="@+id/ImageView6"
												android:layout_width="15dp"
												android:layout_height="15dp"
												android:scaleType="fitCenter"
												android:src="@drawable/heeh_3" />
										</LinearLayout>
									</LinearLayout>

									<LinearLayout
										android:id="@+id/linear14"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:layout_weight="1"
										android:orientation="horizontal"
										android:padding="8dp" />

									<LinearLayout
										android:id="@+id/LinearLayout4"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:layout_marginBottom="10dp"
										android:gravity="center_horizontal"
										android:orientation="horizontal"
										android:padding="1dp">

										<LinearLayout
											android:id="@+id/linear11"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="vertical">

											<ImageView
												android:id="@+id/Phone"
												android:layout_width="23dp"
												android:layout_height="23dp"
												android:layout_margin="2dp"
												android:scaleType="fitCenter"
												android:src="@drawable/phone" />
										</LinearLayout>

										<LinearLayout
											android:id="@+id/linear15"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="vertical">

											<ImageView
												android:id="@+id/Massage"
												android:layout_width="23dp"
												android:layout_height="23dp"
												android:layout_margin="2dp"
												android:scaleType="fitCenter"
												android:src="@drawable/massage" />
										</LinearLayout>

										<LinearLayout
											android:id="@+id/linear16"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="vertical">

											<ImageView
												android:id="@+id/Game"
												android:layout_width="23dp"
												android:layout_height="23dp"
												android:layout_margin="2dp"
												android:scaleType="fitCenter"
												android:src="@drawable/jhgg_1" />
										</LinearLayout>

										<LinearLayout
											android:id="@+id/linear17"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="vertical">

											<ImageView
												android:id="@+id/Translator"
												android:layout_width="23dp"
												android:layout_height="23dp"
												android:layout_margin="2dp"
												android:scaleType="fitCenter"
												android:src="@drawable/jhgg_2" />
										</LinearLayout>

										<LinearLayout
											android:id="@+id/linear18"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="vertical">

											<ImageView
												android:id="@+id/More"
												android:layout_width="23dp"
												android:layout_height="23dp"
												android:layout_margin="2dp"
												android:scaleType="fitCenter"
												android:src="@drawable/ic_apps_white" />
										</LinearLayout>
									</LinearLayout>
								</LinearLayout>

								<ImageView
									android:id="@+id/imageview3"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:scaleType="centerCrop"
									android:src="@drawable/default_image" />
							</FrameLayout>
						</LinearLayout>
					</LinearLayout>

					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">

					</LinearLayout>

					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:alpha="0.5"
						android:padding="8dp"
						android:singleLine="true"
						android:text="Thanks HK's Launcher3 project for home layout!"
						android:textColor="@color/textprimary"
						android:textSize="12sp"
						android:textStyle="italic" />

				</LinearLayout>

				<LinearLayout
					android:id="@+id/linearpfppreview"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="6dp"
					android:background="@drawable/roundedbgviolent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical"
					android:padding="8dp">

					<LinearLayout
						android:id="@+id/linearpfpprevbg"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:background="@drawable/roundedbg"
						android:orientation="vertical"
						android:padding="16dp">

						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_weight="0"
							android:orientation="horizontal">

							<LinearLayout
								android:id="@+id/pfpClipper"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_weight="0"
								android:background="@drawable/roundedbg"
								android:clipChildren="true"
								android:orientation="horizontal">

								<ImageView
									android:id="@+id/imageviewpfp"
									android:layout_width="50dp"
									android:layout_height="50dp"
									android:layout_weight="0"
									android:scaleType="centerCrop"
									app:srcCompat="@android:mipmap/sym_def_app_icon" />
							</LinearLayout>

							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginLeft="8dp"
								android:layout_weight="0"
								android:orientation="vertical">

								<TextView
									android:id="@+id/textView14"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="AGreatfulDisplayName"
									android:textColor="@color/textprimary"
									android:textSize="17sp"
									android:textStyle="bold" />

								<TextView
									android:id="@+id/textView20"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:singleLine="false"
									android:text="A wonderful message that attempts to wrap lines just so you see the amazing work"
									android:textColor="@color/textprimary" />
							</LinearLayout>
						</LinearLayout>

						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:layout_weight="0"
							android:orientation="horizontal">

							<LinearLayout
								android:id="@+id/linearanotherpfpclipper"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_weight="0"
								android:background="@drawable/roundedbg"
								android:clipChildren="true"
								android:orientation="horizontal">

								<ImageView
									android:id="@+id/imageViewPrePfp"
									android:layout_width="50dp"
									android:layout_height="50dp"
									android:layout_weight="0"
									app:srcCompat="@drawable/chatpfp" />
							</LinearLayout>

							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginLeft="8dp"
								android:layout_weight="0"
								android:orientation="vertical">

								<TextView
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Someone Else"
									android:textColor="@color/textprimary"
									android:textSize="17sp"
									android:textStyle="bold" />

								<TextView
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:singleLine="false"
									android:text="Whoa, that image of yours look gorgeous!"
									android:textColor="@color/textprimary" />

								<LinearLayout
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginTop="4dp"
									android:background="@drawable/roundedbgviolent"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="horizontal"
									android:padding="6dp">

									<LinearLayout
										android:id="@+id/reactionpfpclipper"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:layout_weight="0"
										android:background="@drawable/roundedbgviolent"
										android:orientation="horizontal">

										<ImageView
											android:id="@+id/imageviewreactionpfp"
											android:layout_width="20dp"
											android:layout_height="20dp"
											android:layout_weight="0"
											android:scaleType="centerCrop"
											app:srcCompat="@android:mipmap/sym_def_app_icon" />
									</LinearLayout>

									<TextView
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:layout_marginLeft="4dp"
										android:layout_weight="1"
										android:text="❤️" />
								</LinearLayout>
							</LinearLayout>
						</LinearLayout>

						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_marginTop="8dp"
							android:layout_weight="0"
							android:orientation="horizontal">

							<LinearLayout
								android:id="@+id/pfpClipper3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_weight="0"
								android:background="@drawable/roundedbg"
								android:clipChildren="true"
								android:orientation="horizontal">

								<ImageView
									android:id="@+id/imageviewpfp3"
									android:layout_width="50dp"
									android:layout_height="50dp"
									android:layout_weight="0"
									android:scaleType="centerCrop"
									app:srcCompat="@android:mipmap/sym_def_app_icon" />
							</LinearLayout>

							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginLeft="8dp"
								android:layout_weight="0"
								android:orientation="vertical">

								<TextView
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="AGreatfulDisplayName"
									android:textColor="@color/textprimary"
									android:textSize="17sp"
									android:textStyle="bold" />

								<TextView
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:singleLine="false"
									android:text="Why thank you! I'm flattared!"
									android:textColor="@color/textprimary" />

								<LinearLayout
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:layout_marginTop="4dp"
									android:orientation="horizontal">

									<LinearLayout
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:layout_marginRight="8dp"
										android:background="@drawable/roundedbgviolent"
										android:gravity="center_horizontal|center_vertical"
										android:orientation="horizontal"
										android:padding="6dp">

										<LinearLayout
											android:id="@+id/linearprereact"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_weight="0"
											android:background="@drawable/roundedbgviolent"
											android:orientation="horizontal">

											<ImageView
												android:id="@+id/imageviewprereact"
												android:layout_width="20dp"
												android:layout_height="20dp"
												android:layout_weight="0"
												android:scaleType="centerCrop"
												app:srcCompat="@drawable/chatpfp" />
										</LinearLayout>

										<TextView
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_marginLeft="4dp"
											android:layout_weight="1"
											android:text="💖" />
									</LinearLayout>

									<LinearLayout
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:layout_marginRight="8dp"
										android:background="@drawable/roundedbgviolent"
										android:gravity="center_horizontal|center_vertical"
										android:orientation="horizontal"
										android:padding="6dp">

										<TextView
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_weight="1"
											android:alpha="0.5"
											android:text="+1 thread" />
									</LinearLayout>
								</LinearLayout>

							</LinearLayout>
						</LinearLayout>

					</LinearLayout>
				</LinearLayout>

				<TextView
					android:id="@+id/textView3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="4dp"
					android:text="@string/WallColors"
					android:textColor="@color/textprimary"
					android:textSize="20sp"
					android:textStyle="bold" />

				<LinearLayout
					android:id="@+id/colorpreviews"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="6dp"
					android:background="@drawable/roundedbgviolent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:padding="8dp">

					<androidx.cardview.widget.CardView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:layout_marginRight="8dp"
						app:cardCornerRadius="200dp"
						app:cardElevation="0dp">

						<LinearLayout
							android:id="@+id/color1"
							android:layout_width="45dp"
							android:layout_height="45dp"
							android:orientation="vertical"></LinearLayout>
					</androidx.cardview.widget.CardView>

					<androidx.cardview.widget.CardView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:layout_marginRight="8dp"
						app:cardCornerRadius="200dp"
